<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<type-mapping>
		<sql-type jdbc-type="INTEGER" hibernate-type="int"
			not-null="true">
		</sql-type>
		<sql-type jdbc-type="VARCHAR" hibernate-type="string"
			length="45" not-null="false">
		</sql-type>
	</type-mapping>
	<table-filter match-schema="payu" match-name=".*"></table-filter>
<table catalog="payu" name="payu_cliente" schema=""></table>
<table catalog="payu" name="payu_transaccion">
		<primary-key></primary-key>
	</table>
	<table catalog="payu" name="payu_pagos_recibidos">
		<primary-key></primary-key>
	</table>
</hibernate-reverse-engineering>